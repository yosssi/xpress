{
  "watchers": [{
    "extension": "go",
    "commands": ["make rerun"]
  }, {
    "extension": "styl",
    "commands": ["make stylus"]
  }, {
    "extension": "css",
    "excludes": ["all.css", "all.min.css"],
    "commands": ["make catcss", "make uglifycss"]
  }, {
    "extension": "js",
    "excludes": ["all.js", "all.min.js"],
    "commands": ["make catjs", "make uglifyjs"]
  }]
}
